MYSQL_ROOT_PASSWORD=_RANDOM16_
MYSQL_USER_PASSWORD=_RANDOM16_

CTF_ADMIN_ACCOUNT=admin
CTF_ADMIN_UID=_RANDOM16_
CTF_ADMIN_PASSWORD=_RANDOM16_


# CTF_SCOREBOARD_AFF
# all       : all users
# user_only : only logged user, or joined CTF
CTF_SCOREBOARD_AFF=all


# CTF_LOCALE_ENABLED
# true : let user change langage (english)
# false : french only
CTF_LOCALE_ENABLED=false


# CTF_REQUIRE_EMAIL_VALIDATION
#
# true  : User account enabled after mail validation. CTF_MAIL_ENABLED must be true, CTF_MAIL_XXXX must be set
# false : User account enabled withour mail validation
CTF_REQUIRE_EMAIL_VALIDATION=false
CTF_MAIL_ENABLED=false
CTF_MAIL_USERNAME=yoloctf
CTF_MAIL_PASSWD=passwdtobeset
CTF_MAIL_FROMMAIL=yoloctf@gmail.com
CTF_MAIL_FROMNAME=YoloCTF


# CTF_REGISTER_CODE
#
# Not set or empty : no code needed to register
# CTF_REGISTER_CODE=YOLO


